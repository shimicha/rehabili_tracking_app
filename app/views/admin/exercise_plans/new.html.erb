<h1>実施訓練メニューの新規作成</h1>

<%= form_with model: [:admin, @exercise_plan], local: true do |f| %>
  <div class="field">
    <%= f.label :training_menu %>
    <%= f.text_field :training_menu %>
  </div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>

  <div class="field">
  <%= f.submit "作成する", class: "submit-btn" %>
  </div>
<% end %>
